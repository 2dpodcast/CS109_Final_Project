var ta_AD_FAIL_MS = 4000;
var ta_ldrLogFails = 0;
function ta_logLatency(s,t) {
if(typeof ta.util.error.record != "undefined" && typeof ta.common.ads != "undefined") {
ta.common.ads.scrapeAndLog('ad728X90',s,t);
} else {            if(window.ta_ldrLogFail < 3) {
window.ta_ldrLogFail++;
setTimeout(function(){
ta_logLatency(s,t)
}, 3000);
}
}
}
var ta_ldrStart = new Date().getTime();
var ta_ldrFail = setTimeout(function(){ta_logLatency('4s/Fail')}, window.ta_AD_FAIL_MS);
</script>
