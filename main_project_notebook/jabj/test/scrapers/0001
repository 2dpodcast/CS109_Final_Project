<script type="text/javascript">
function showCascadeSearch(sHotelID, bFromPop, sLinkType, sArea) {
if(sHotelID == null || sHotelID == '') {return;}
var tgtUrl = "/HotelDateSearch?d=" + sHotelID + "&returnTo=__2F__ForumHome&fromPop=" + bFromPop;
tgtUrl = tgtUrl + "&area=" + sArea;
var sFromSource = "ForumHome";
if(sFromSource !="") {
if(sLinkType && sLinkType != "") {sFromSource += sLinkType;}
tgtUrl += "&from=" + sFromSource;
}
var w = window.open ( tgtUrl, "CascadeSearch", "toolbar=0,resizable=1,menubar=0,location=0,status=0,scrollbars=1,width=245,height=610,screenX=5,screenY=5,left=5,top=5");
if (w != null) {
w.opener = self;
w.focus();
}
}
</script>
